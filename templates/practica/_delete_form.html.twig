<form
    method="post"
    action="{{ path('app_practica_delete', {'id': praxis.id}) }}"
    onsubmit="return confirm('Are you sure you want to delete this item?');"
>
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ praxis.id) }}">
    <button class="btn">Delete</button>
</form>
